{
    "name": "artisan-labs/laravel-visit-tracker",
    "version": "dev-stable",
    "description": "An improvement and combination of the 'antonioribeiro/tracker' & 'shetabit/visitor' plugins",
    "keywords": [
        "laravel",
        "visit",
        "tracker",
        "improvement",
        "antonioribeiro/tracker",
        "shetabit/visitor"
    ],
    "homepage": "https://github.com/artisanbr/laravel-visit-tracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Carlos Ribeiro",
            "email": "acr@antoniocarlosribeiro.com",
            "homepage": "https://github.com/antonioribeiro"
        },
        {
            "name": "Mahdi Khanzadi",
            "email": "khanzadimahdi@gmail.com",
            "homepage": "https://github.com/khanzadimahdi"
        },
        {
            "name": "Renalcio Carlos Jr.",
            "homepage": "https://github.com/renalcio",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "canonical": true,
            "url": "packages/pragmarx/*",
            "options": {
                "symlink": true,
                "versions": {
                    "pragmarx/tracker": "4.0.x-dev",
                    "pragmarx/support": "0.9.x-dev"
                }
            }
        }
    ],
    "require": {
        "laravel/framework": ">=8",
        "geoip2/geoip2": "^2.13",
        "shetabit/visitor": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "ArtisanLabs\\LaravelVisitTracker\\": "src/",
            "PragmaRX\\Tracker\\": "packages/pragmarx/tracker/src/",
            "PragmaRX\\Support\\": "packages/pragmarx/support/src/"
        },
        "files": [
            "packages/pragmarx/support/src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.0.x-dev",
            "master": "dev-main",
            "dev": "dev-main"
        },
        "laravel": {
            "providers": [
                "PragmaRX\\Tracker\\Vendor\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Tracker": "PragmaRX\\Tracker\\Vendor\\Laravel\\Facade"
            }
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/stubs"
        ]
    }
}
